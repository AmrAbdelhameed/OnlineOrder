<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_show_brands_to_user"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.amr.onlineorder.ShowUsersOfBrand">


    <android.support.v7.widget.Toolbar xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/categoriesToolBar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/DarkPastelGreen"
        android:popupTheme="@style/ThemeOverlay.AppCompat.Light"
        app:title="Users"
        app:titleTextColor="#ffffff" />


    <ListView
        android:id="@+id/listview_users_admin"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="15dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp">

    </ListView>


</LinearLayout>
